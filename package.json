    "sast": "^0.8.1",
    "sort-object": "^2.0.2",
    "system-commands": "^1.1.7"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.2.0",
    "babel": "^5.8.23",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "jest-plugin-fs": "^2.9.0",
    "jest-transform-stub": "^1.0.0",
